{
  "manifest_version": 2,
  "name": "whatsdarky",
  "description": "Darky mode for WhatsApp Web",
  "author": "gomflo",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://web.whatsapp.com/*"],
      "css": ["styles.css"]
    }
  ]
}
